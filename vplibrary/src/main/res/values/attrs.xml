<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <declare-styleable name="VpSlideBannerView">
        <attr name="isAutoPlay" format="boolean"></attr>
        <attr name="intervalTime" format="integer"></attr>
        <attr name="isShowIndicator" format="boolean"></attr>
        <attr name="indicatorPosition">
            <enum name="left" value="0"></enum>
            <enum name="center" value="1"></enum>
            <enum name="right" value="2"></enum>

        </attr>
    </declare-styleable>
    
</resources>
